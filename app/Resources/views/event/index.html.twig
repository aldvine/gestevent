{% extends 'base.html.twig' %}

{% block body %}
    <ul class="list-group">
        {% for event in events %}
            <li class="list-group-item d-flex justify-content-between align-items-center">
                {{event.title}}
                <span class="badge badge-primary badge-pill">{{event.nbPlace}}</span>
            </li>
        {% endfor %}
    </ul>

{% endblock %}